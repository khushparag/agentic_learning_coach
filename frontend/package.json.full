{
  "name": "agentic-learning-coach-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:stats": "vite build --mode production --reporter=verbose",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:security": "eslint . --ext ts,tsx --config .eslintrc.security.js",
    "type-check": "tsc --noEmit",
    "perf:audit": "lighthouse http://localhost:3000 --output=html --output-path=./performance-audit.html",
    "perf:monitor": "node scripts/performance-monitor.js",
    "test": "jest",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headless": "cypress run --headless",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173 test:e2e",
    "test:e2e:staging": "cypress run --config baseUrl=https://staging.agentic-learning-coach.com",
    "test:e2e:production:critical": "cypress run --spec 'cypress/e2e/critical-user-flows.cy.ts' --config baseUrl=https://agentic-learning-coach.com",
    "test:visual": "percy exec -- cypress run",
    "test:accessibility": "npm run build && npm run preview & npx wait-on http://localhost:4173 && npx @axe-core/cli http://localhost:4173 --exit",
    "test:performance": "node src/test/performance/bundle-analysis.test.ts",
    "test:all": "npm run test:ci && npm run test:e2e:ci",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:test": "test-storybook",
    "chromatic": "npx chromatic --project-token=your-project-token",
    "start:mock-api": "node scripts/mock-api-server.js",
    "quality:gates": "node scripts/quality-gates.js",
    "quality:check": "node scripts/quality-check.js",
    "security:scan": "npm audit --audit-level moderate && detect-secrets scan --all-files --baseline .secrets.baseline",
    "security:secrets": "detect-secrets scan --all-files --baseline .secrets.baseline",
    "security:check": "npm audit --audit-level moderate",
    "deploy:staging": "node scripts/deploy.js staging",
    "deploy:production": "node scripts/deploy.js production",
    "deploy:production:full": "node scripts/production-deploy.js",
    "integration:test": "node scripts/integration-test.js",
    "dev:setup": "node scripts/dev-setup.js",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "bundle:analyze": "npm run build && npx webpack-bundle-analyzer dist/stats.json",
    "bundle:size": "bundlesize",
    "analyze:bundle": "npm run build && npx vite-bundle-analyzer dist",
    "check:bundle-size": "bundlesize",
    "lighthouse:ci": "lhci autorun",
    "wait-for-services": "wait-on http://localhost:8000/health --timeout 60000",
    "wait-for-app": "wait-on http://localhost:4173 --timeout 30000",
    "health": "echo 'OK'"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "@tanstack/react-query": "^5.8.4",
    "zustand": "^4.4.6",
    "axios": "^1.6.2",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "framer-motion": "^10.16.5",
    "@monaco-editor/react": "^4.6.0",
    "recharts": "^2.8.0",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-no-secrets": "^0.8.9",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-bundle-analyzer": "^0.7.0",
    "tailwindcss": "^3.3.5",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "rollup-plugin-visualizer": "^5.9.2",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-a11y": "^7.6.3",
    "@storybook/addon-viewport": "^7.6.3",
    "@storybook/addon-docs": "^7.6.3",
    "@storybook/blocks": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/react-vite": "^7.6.3",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/test-runner": "^0.15.2",
    "storybook": "^7.6.3",
    "cypress": "^13.6.1",
    "start-server-and-test": "^2.0.3",
    "@percy/cli": "^1.27.4",
    "@percy/cypress": "^3.1.2",
    "chromatic": "^10.1.0",
    "msw": "^2.0.8",
    "@faker-js/faker": "^8.3.1",
    "factory.ts": "^1.4.0",
    "jest-axe": "^8.0.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-storybook": "^0.6.15",
    "@axe-core/cli": "^4.8.2",
    "pa11y": "^7.0.0",
    "bundlesize": "^0.18.1",
    "license-checker": "^25.0.1",
    "npm-check-updates": "^16.14.11",
    "detect-secrets": "^1.4.0",
    "prettier": "^3.1.0",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0",
    "http-server": "^14.1.1",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  }
}